# Copy this file to config.yaml in the root directory
# Then create a .env file with your API keys:
# OPENAI_API_KEY=your_key_here
# REPLICATE_API_TOKEN=your_token_here

llm_api:
  url: "https://api.openai.com"
  model: "gpt-4o"
  api_key: "${OPENAI_API_KEY}"  # Will be replaced with value from .env

diffusion:
  api:
    url: "https://api.replicate.com"
    model: "black-forest-labs/flux-dev"
    api_key: "${REPLICATE_API_TOKEN}"  # Will be replaced with value from .env
  aspect_ratio: "16:9"
  megapixels: 1.0

default_keywords:
  - "nature"
  - "futuristic"
  - "cyberpunk"
  - "photorealistic"
